<Page x:Class="TrafficSystem.SetupScreen"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TrafficSystem"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SetupScreen">

    <Grid x:Name="MyGrid" Background="White">
        <Button x:Name="Run_Button" Content="Run_Button" HorizontalAlignment="Left" Margin="572,245,0,0" VerticalAlignment="Top" Height="45" Width="86"/>
        <Label Content="Number Of Cars" HorizontalAlignment="Left" Margin="140,120,0,0" VerticalAlignment="Top"/>
        <Label Content="Highway Width" HorizontalAlignment="Left" Margin="140,140,0,0" VerticalAlignment="Top"/>
        <StackPanel x:Name="Stack_Panel"/>


    </Grid>
</Page>
